{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["dist"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./docs/",
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}
